{
    "name": "canvas/core",
    "description": "Saas Kanvas Ecosystem Packages",
    "license": "MIT",
    "authors": [{
        "name": "kaioken",
        "email": "max@mctekk.com"
    }],
    "require": {
        "php": ">=7.2",
        "ext-phalcon": ">=3.4",
        "baka/baka": "0.5.2",
        "dariuszp/cli-progress-bar": "^1.0",
        "dmkit/phalcon-jwt-auth": "dev-master",
        "duncan3dc/fork-helper": "^2.2.0",
        "firebase/php-jwt": "^5.0",
        "hybridauth/hybridauth": "dev-master",
        "lcobucci/jwt": "^3.2",
        "league/flysystem": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/fractal": "^0.17.0",
        "mark-gerarts/auto-mapper-plus": "2.0.0-alpha1",
        "monolog/monolog": "^1.23",
        "namshi/notificator": " 1.0.*",
        "nesbot/carbon": "~1.18",
        "norkunas/onesignal-php-api": "^1.12",
        "phalcon/incubator": "~3.4",
        "php-amqplib/php-amqplib": "^2.9",
        "php-http/guzzle6-adapter": "1.1",
        "psr/log": "^1.0",
        "pusher/pusher-php-server": "3.4.1",
        "robmorgan/phinx": "0.*",
        "sentry/sentry": "^1.9",
        "sly/notification-pusher": "2.x",
        "vlucas/phpdotenv": "^2.4",
        "bakaphp/php-apple-signin": "^2.0"
    },
    "require-dev": {
        "codeception/verify": "*",
        "codeception/codeception": "2.4",
        "phpstan/phpstan": "^0.9.2",
        "squizlabs/php_codesniffer": "^3.2",
        "odan/phinx-migrations-generator": "4.*.*"
    },
    "autoload": {
        "psr-4": {
            "Canvas\\": "src/"
        },
        "files": [
            "src/Core/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Canvas\\": "src/",
            "Canvas\\Tests\\": "tests/"
        },
        "files": [
            "src/Core/functions.php"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "discard-changes": true
    },
    "scripts": {
        "phpcs": "phpcs --standard=PSR2"
    },
    "prefer-stable": true
}
