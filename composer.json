{
    "name": "canvas/core",
    "description": "Saas Kanvas Ecosystem Packages",
    "license": "MIT",
    "authors": [{
        "name": "kaioken",
        "email": "max@mctekk.com"
    }],
    "require": {
        "php": ">=7.4",
        "ext-phalcon": ">=4",
        "baka/baka": "0.7.x-dev",
        "dariuszp/cli-progress-bar": "^1.0",
        "elasticsearch/elasticsearch": "^7.5",
        "firebase/php-jwt": "^5.0",
        "fzaninotto/faker": "^1.9@dev",
        "lcobucci/jwt": "^3.2",
        "league/flysystem": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/fractal": "^0.17.0",
        "mark-gerarts/auto-mapper-plus": "2.0.0-alpha1",
        "monolog/monolog": "^2.1",
        "nesbot/carbon": "^2.3",
        "odan/phinx-migrations-generator": "^5",
        "phalcon/ide-stubs": "^4.0",
        "phalcon/incubator": "4.0.x-dev",
        "phalcon/incubator-acl": "^1.0.0-alpha.1",
        "phalcon/incubator-test" : "^1.0.0-alpha.1",
        "php-amqplib/php-amqplib": "^2.9",
        "php-http/guzzle6-adapter": "^2.0",
        "psr/log": "^1.0",
        "pusher/pusher-php-server": "^4.1",
        "norkunas/onesignal-php-api": "^1.12",
        "sentry/sentry": "^2.4",
        "phpstan/phpstan": "0.9.3",
        "phpunit/phpunit": "^9.1",
        "robmorgan/phinx": "^0.12",
        "sly/notification-pusher": "2.x",
        "squizlabs/php_codesniffer": "^3.2",
        "stripe/stripe-php": "^7.36",
        "swiftmailer/swiftmailer": "^6",
        "symfony/browser-kit": "3.4",
        "symfony/config": "^4.4",
        "symfony/console": "^4.4",
        "symfony/css-selector": "3.4",
        "symfony/debug": "^4.4",
        "symfony/dom-crawler": "3.4",
        "symfony/event-dispatcher": "^4.4",
        "symfony/filesystem": "^4.4",
        "symfony/finder": "^5.1",
        "symfony/process": "^4.4",
        "symfony/yaml": "3.4",
        "vlucas/phpdotenv": "^4.1"
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.2",
        "codeception/module-cli": "^1.0.0",
        "codeception/module-filesystem": "^1.0",
        "codeception/module-phalcon": "^1.0",
        "codeception/module-phalcon4": "^1.0",
        "codeception/module-rest": "^1.2",
        "phpstan/phpstan": "^0.9.2",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Canvas\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Canvas\\": "src/",
            "Canvas\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "discard-changes": true
    },
    "scripts": {
        "phpcs": "phpcs --standard=PSR2"
    },
    "prefer-stable": true
}
